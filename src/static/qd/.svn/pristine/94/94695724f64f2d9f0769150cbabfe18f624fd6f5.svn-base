/*
* Clearfix
*
* Clears floats inside the container
*/
.lbf-numberspinner {
  position: relative;
  padding: 0 24px;
  display: inline-block;
  *display: inline;
  *zoom: 1; }
  .lbf-numberspinner .lbf-numberspinner-input {
    width: 48px;
    text-align: center;
    border-color: #D9D9D9;
    *margin-top: -1px; }
    .lbf-numberspinner .lbf-numberspinner-input:hover, .lbf-numberspinner .lbf-numberspinner-input:focus {
      border-color: #D9D9D9; }
  .lbf-numberspinner .lbf-numberspinner-decrease, .lbf-numberspinner .lbf-numberspinner-increase {
    position: absolute;
    top: 0;
    border-color: #D9D9D9;
    width: 23px;
    padding: 4px 0 4px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    -webkit-box-shadow: none;
    box-shadow: none;
    border-radius: 0; }
  .lbf-numberspinner .lbf-numberspinner-decrease {
    left: 0;
    border-right: 0 none; }
  .lbf-numberspinner .lbf-numberspinner-increase {
    right: 0;
    border-left: 0 none; }

